{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "article_id": {
      "type": "uid"
    },
    "url": {
      "type": "string",
      "regex": "^(https?|chrome):\\/\\/[^\\s$.?#].[^\\s]*$",
      "unique": true,
      "required": true
    },
    "title": {
      "type": "string",
      "required": true
    },
    "published": {
      "type": "date"
    }
  }
}
